# Configurações do Albion.PortBridge
# Copie este arquivo para .env e edite com suas configurações

# =============================================================================
# CONFIGURAÇÕES PRINCIPAIS
# =============================================================================

# Porta local onde o proxy escuta
LOCAL_FORWARD_PORT=5151

# Porta pública exposta (para ngrok)
PUBLIC_EXPOSE_PORT=5151

# =============================================================================
# NGROK (OPCIONAL - para acesso externo via internet)
# =============================================================================

# Token de autenticação do ngrok
# Obtenha em: https://dashboard.ngrok.com/get-started/your-authtoken
NGROK_AUTHTOKEN=your_ngrok_auth_token_here

# =============================================================================
# FRP UDP (RECOMENDADO - para infraestrutura local)
# =============================================================================

# IP do segundo PC onde roda o servidor FRP
FRP_SERVER_IP=192.168.18.31

# Porta do servidor FRP
FRP_SERVER_PORT=6000

# Porta do túnel UDP (onde clientes se conectam)
FRP_TUNNEL_PORT=15151

# Porta local do serviço Crypto/Sniffer
FRP_LOCAL_PORT=5050

# Token de autenticação FRP (deve ser igual no cliente e servidor)
FRP_TOKEN=albion123

# =============================================================================
# CONFIGURAÇÕES AVANÇADAS
# =============================================================================

# Habilitar criptografia FRP
FRP_USE_ENCRYPTION=true

# Habilitar compressão FRP
FRP_USE_COMPRESSION=true

# Porta do dashboard FRP (opcional)
FRP_DASHBOARD_PORT=7500

# Usuário do dashboard FRP
FRP_DASHBOARD_USER=admin

# Senha do dashboard FRP
FRP_DASHBOARD_PASSWORD=admin123
